{
  "version": 2,
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/web/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY": "@clerk_publishable_key",
    "CLERK_SECRET_KEY": "@clerk_secret_key",
    "DATABASE_URL": "@database_url",
    "POSTGRES_URL": "@postgres_url",
    "NEXT_PUBLIC_APP_URL": "https://aeonprotocol.com",
    "NEXT_PUBLIC_SITE_URL": "https://aeonprotocol.com",
    "NEXT_PUBLIC_BACKEND_URL": "http://159.223.198.119:8000",
    "NEXT_PUBLIC_STACK_PROJECT_ID": "@stack_project_id",
    "NEXT_PUBLIC_STACK_PUBLISHABLE_CLIENT_KEY": "@stack_publishable_key",
    "STACK_SECRET_SERVER_KEY": "@stack_secret_key"
  },
  "functions": {
    "apps/web/app/api/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  }
}
